{
    "id": "add",
    "summary": "Addition of two numbers",
    "description": "Sums up two numbers and returns the computed sum.\n\nNo-data values are taken into account so that `null` is returned if any element is such a value.\n\nThe computations follow [IEEE Standard 754](https://ieeexplore.ieee.org/document/8766229) whenever the processing environment supports it.",
    "categories": [
        "math"
    ],
    "parameter_order": [
        "a",
        "b"
    ],
    "parameters": {
        "a": {
            "description": "The first summand.",
            "schema": {
                "type": [
                    "number",
                    "null"
                ]
            },
            "required": true
        },
        "b": {
            "description": "The second summand.",
            "schema": {
                "type": [
                    "number",
                    "null"
                ]
            },
            "required": true
        }
    },
    "returns": {
        "description": "The computed sum of the two numbers.",
        "schema": {
            "type": [
                "number",
                "null"
            ]
        }
    },
    "examples": [
        {
            "arguments": {
                "a": 5,
                "b": 2.5
            },
            "returns": 7.5
        },
        {
            "arguments": {
                "a": -2,
                "b": -4
            },
            "returns": -6
        },
        {
            "arguments": {
                "a": 1,
                "b": null
            },
            "returns": null
        }
    ],
