{
    "id": "get_result",
    "summary": "Load batch job results by id",
    "description": "Load internal batch job results stored by the authenticated user from the current back-end by job id.",
    "categories": [
        "cubes",
        "import"
    ],
    "parameters": {
        "id": {
            "description": "The job id.",
            "schema": {
                "type": "string",
                "pattern": "^[A-Za-z0-9_\\-\\.~]+$"
            },
            "required": true
        }
    },
    "returns": {
        "description": "A data cube for further processing.",
        "schema": {
            "oneOf": [
                {
                    "type": "object",
                    "format": "raster-cube"
                },
                {
                    "type": "object",
                    "format": "vector-cube"
                }
            ]
        }
    }
}